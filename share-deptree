target=$1
shift
if test $# -gt 0; then
    indent=$1
    shift
else
    indent=""
fi
echo "${indent}${target}" | sed 's,#,	,g;s,␣, ,g'
depdir=${target}.dep
if test -d $depdir; then
    for dep in $(cd ${depdir}; cat * | sed 's, ,␣,g' | sort); do
        cwd=$(pwd)
        cd "$(dirname '$dep')"
        share-deptree "$(basename $dep)" "${indent}#"
        cd $cwd
    done
fi
