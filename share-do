. header
target=$1
shift
logfile=${logfile-}
if test -z "$logfile"; then
    export logfile=${target}.do.log
fi
base=$(echo $target | sed 's \.[^.]\+$  ')
if test -x ${target}.do.do; then
    log exec=true $echo share-do ${base}.do
fi
if test -x ${target}.do; then
    if share-chkdep $target; then
        . share-setenv $target
        log exec=true $echo flock ${target}.lock -c "sh -x ${target}.do $base" 
    fi
else
    log -- "$target: no script"
fi
