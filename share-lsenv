. header
for env in *.env; do
    for var in $(ls $env); do
        target=$(echo $env | sed 's \.env$  ')
        cat ${env}/${var} | sed "s,^,${target}	${var}	,"
    done
done >${tmp}
case $# in
    0)
        cat $tmp
        ;;
    1)
        awk "\$1 ~ /^$1$/{print \$0}" $tmp
        ;;
esac
