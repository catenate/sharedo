case $# in
    1)
        rm -rf ${1}.dep
        ;;
    2)
        dep_quoted=$(echo "$2" | sed 's / \\/ g')
        file=$(grep -l "$dep_quoted" ${1}.dep/* 2>/dev/null)
        if test -n "$file"; then
            sed -i "/^${dep_quoted}$/d" $file
        fi
        ;;
esac
