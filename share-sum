if echo "$*" | grep '^\$' >/dev/null 2>&1; then
    eval "echo $*" | md5sum
    exit 0
fi
if echo "$*" | grep '␣' >/dev/null 2>&1; then
    eval $(echo $* | sed 's,␣, ,g') | md5sum
    exit 0
fi
if test -d "$*"; then
    cd "$*"
    ls -l | md5sum
    exit 0
fi
if test -r "$*"; then
    cat "$*" | md5sum
    exit 0
fi
echo 0
exit 0
