if echo "$*" | grep '^\$' >/dev/null 2>&1; then
    eval "echo $*" | md5sum
else
    if echo "$*" | grep '␣' >/dev/null 2>&1; then
        eval $(echo $* | sed 's,␣, ,g') | md5sum
    else
        if test -r "$*"; then
            cat "$*" | md5sum
        else
            echo 0
        fi
    fi
fi
exit 0
